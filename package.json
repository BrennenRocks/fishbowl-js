{
  "name": "fishbowl-js",
  "version": "2.5.2",
  "description": "A JavaScript wrapper for the Fishbowl API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrennenRocks/fishbowl-js.git"
  },
  "keywords": [
    "fishbowl",
    "inventory",
    "fishbowljs",
    "js-fishbowl",
    "node-fishbowl",
    "fishbowlnode",
    "plugin"
  ],
  "author": "Brennen Davis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrennenRocks/fishbowl-js/issues"
  },
  "homepage": "https://github.com/BrennenRocks/fishbowl-js#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/node": "^10.12.24",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "winston": "^3.2.1",
    "jquery-csv": "^1.0.3"
  }
}
